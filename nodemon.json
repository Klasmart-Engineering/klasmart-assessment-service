{
  "restartable": "rs",
  "ignore": [".git", "node_modules"],
  "verbose": true,
  "execMap": {
    "ts": "node --trace-warnings --require ts-node/register",
    "ts-node": "node --trace-warnings --require ts-node/register"
  },
  "watch": ["./src"],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json,ts"
}
